{
  "main": "index",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.3.2",
  "sideEffects": [
    "*.css"
  ],
  "resolutions": {
    "@metamask/sdk": "0.33.1",
    "@metamask/utils": "11.8.1",
    "@stoplight/spectral-core": "1.20.0",
    "@walletconnect/core": "2.21.10",
    "@walletconnect/sign-client": "2.21.10",
    "@walletconnect/universal-provider": "2.21.10",
    "@walletconnect/utils": "2.21.10",
    "@walletconnect/types": "2.21.10",
    "ajv": "8.17.1",
    "react-native": "0.81.5",
    "react-native-web": "0.21.0",
    "tamagui*": "1.139.2",
    "ox": "0.9.14",
    "viem": "2.39.3"
  },
  "engines": {
    "node": ">=25"
  },
  "scripts": {
    "start": "expo start --dev-client --clear",
    "dev": "bun run ./scripts/dev.ts",
    "dev:e2e": "expo start --web",
    "dev:clear": "expo start --clear --web",
    "build": "expo export --platform web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "serve": "bun --env-file=.env expo-server",
    "serve:e2e": "bun --env-file=.env expo-server",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit && bun gql-tada check",
    "gql:check": "bun gql-tada check",
    "clean:watchman": "watchman watch-del-all",
    "clean": "rm -rf dist && rm -rf .expo && rm -rf .turbo && rm -rf .tamagui && rm -rf node_modules && rm -rf tsconfig.tsbuildinfo && rm -rf tamagui-web.css",
    "atlas:export": "EXPO_UNSTABLE_ATLAS=true npx expo export --clear --platform web",
    "atlas:open": "npx expo-atlas .expo/atlas.jsonl",
    "test": "jest",
    "local:proxy": "bun run scripts/local-auth-proxy.ts"
  },
  "dependencies": {
    "@biconomy/abstractjs": "^1.1.19",
    "@metamask/sdk": "~0.33.1",
    "@ory/client-fetch": "^1.16.6",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@tamagui/babel-plugin": "1.139.2",
    "@tamagui/config": "1.139.2",
    "@tamagui/metro-plugin": "1.139.2",
    "@tanstack/react-query": "^5.89.0",
    "@walletconnect/ethereum-provider": "~2.21.1",
    "@zxcvbn-ts/core": "^3.0.4",
    "burnt": "^0.13.0",
    "expo": "54.0.27",
    "expo-build-properties": "~1.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.17",
    "expo-secure-store": "~15.0.8",
    "expo-server": "~1.0.5",
    "expo-splash-screen": "~31.0.12",
    "expo-web-browser": "~15.0.10",
    "launchdarkly-react-client-sdk": "^3.8.1",
    "lodash": "^4.17.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "0.21.0",
    "react-native-webview": "13.15.0",
    "tamagui": "1.139.2",
    "viem": "2.39.3",
    "wagmi": "3.0.1",
    "zod": "4.1.12",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@tanstack/react-query-devtools": "^5.89.0",
    "expo-atlas": "^0.4.3"
  }
}
